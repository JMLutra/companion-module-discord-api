{
	"name": "discord-api",
	"version": "0.1.2",
	"api_version": "1.0.0",
	"keywords": [
		"Software",
		"Chat"
	],
	"manufacturer": "Discord",
	"product": "API",
	"shortname": "discord",
	"description": "Discord",
	"main": "dist/index.js",
	"scripts": {
		"dev": "yarn build:watch",
		"build": "npx rimraf dist && npx typescript@~4.6 -p tsconfig.build.json",
		"build:main": "tsc -p tsconfig.build.json",
		"build:watch": "tsc -p tsconfig.build.json --watch",
		"lint": "eslint . --ext .ts",
		"postinstall": "yarn build",
		"test": "jest"
	},
	"author": "Jeff Martin <dist@dist.dev> (https://dist.dev)",
	"contributors": [],
	"homepage": "https://github.com/thedist/companion-module-discord-api#readme",
	"bugs": {
		"url": "https://github.com/thedist/companion-module-discord-api/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/bitfocus/companion-module-discord-api.git"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,json,md}": [
			"prettier --write",
			"git add"
		],
		"*.{ts}": [
			"yarn lint --fix --",
			"git add"
		]
	},
	"eslintIgnore": [
		"/tests/*.test.ts"
	],
	"license": "MIT",
	"dependencies": {
		"got-cjs": "^12.0.1"
	},
	"devDependencies": {
		"@types/jest": "^27.4.0",
		"@types/node": "^17.0.12",
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.10.1",
		"eslint": "^8.7.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"husky": "^7.0.4",
		"jest": "^27.4.7",
		"lint-staged": "^12.3.2",
		"prettier": "^2.5.1",
		"rimraf": "^3.0.2",
		"ts-jest": "^27.1.3",
		"typescript": "~4.6"
	}
}
